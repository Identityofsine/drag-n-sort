{
  "name": "drag-n-sort",
  "version": "1.4.0",
  "description": "A React component designed to easily allow elements to be dragged and dropped within one container for sorting purposes.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
		"build:sass": "sass src/styles/index.scss dist/css/index.css",
		"postbuild": "cp -r src/styles dist/",
		"test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Identityofsine/drag-n-sort.git"
  },
  "keywords": [
    "react",
    "sorter",
    "react-component",
    "react-dom",
    "front-end",
    "typescript",
    "ui-element"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "author": "fofx.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Identityofsine/drag-n-sort/issues"
  },
  "homepage": "https://github.com/Identityofsine/drag-n-sort#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "tslib": "^2.6.2",
    "@types/react": "^18.2.73",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.72.0"
  }
}
